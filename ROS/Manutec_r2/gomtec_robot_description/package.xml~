<package>
  <name>gomtec_robot_description</name>
  <version>1.1.0</version>
  <description>
    This package contains the mechanical, kinematic and visual description 
    of the gomtec Roberta robot, the gomtec RoboCommander teach-in device 
    and the gomtec gripper with and without integrated camera system.
  </description>
  <author>Gomtec GmbH</author>
  <maintainer email="ros@gomtec.de">Gomtec GmbH</maintainer>
  <license>BSD</license>

  <url type="website">http://wiki.ros.org/Robots/Roberta</url>
  <url type="bugtracker">https://github.com/gomtec/roberta-ros/issues</url>
  <url type="repository">https://github.com/gomtec/roberta-ros</url>
  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <run_depend>roscpp</run_depend>
  <build_depend>std_msgs</build_depend>
  <run_depend>tf</run_depend>
  <build_depend>tf</build_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>joint_state_publisher</run_depend>
  <run_depend>robot_state_publisher</run_depend>
  <run_depend>rviz</run_depend>
  <run_depend>xacro</run_depend>

  <export>
    <architecture_independent />
  </export>
</package>
